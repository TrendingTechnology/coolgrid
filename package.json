    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.0-beta.6",
    "coveralls": "^3.0.2",
    "cpr": "^3.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
