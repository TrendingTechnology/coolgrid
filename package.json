{
  "name": "coolgrid",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "maintainers": [
    "Vit Bokisch <vit@bokisch.cz>"
  ],
  "version": "0.4.0",
  "description": "Ultra flexible and extensible grid system based on styled-components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitus-labs/coolgrid"
  },
  "main": "dist/coolgrid.js",
  "umd:main": "dist/coolgrid.umd.js",
  "module": "dist/coolgrid.module.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf lib dist && babel src -d lib && cpr lib dist --delete-first && yarn rollup",
    "storybook": "start-storybook -p 6006 -c tools/storybook",
    "storybook:build": "build-storybook -c tools/storybook -o docs/",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "lint": "yarn lint:css && yarn lint:ts",
    "lint:css": "stylelint './src/**/*.ts*'",
    "lint:ts": "tslint -p ./tsconfig.json \"./src/**/*.ts*\"",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "cover": "coveralls < .coverage/lcov.info",
    "rollup": "rollup -c",
    "pkgs": "rm -rf node_modules/ package-lock.json yarn.lock && yarn",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "react": ">= 16.3",
    "styled-components": ">= 4"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-notes": "^5.0.0",
    "@storybook/addon-storysource": "^5.0.0",
    "@storybook/addon-viewport": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^24.0.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^11.9.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/storybook__addon-notes": "^4.0.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.2",
    "cpr": "^3.0.1",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-styled-components": "^7.0.0-2",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.1",
    "rollup": "^1.1.2",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "^6.0.2",
    "styled-components": "^4.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  }
}
